{
  "name": "akventure",
  "version": "1.0.0",
  "description": "group of all UAE companies",
  "main": "server.js",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
